{
  "name": "react-whiteboard-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "json-server": "^0.14.0",
    "react": "^16.4.1",
    "react-content-loader": "^3.1.2",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "styled-components": "^3.4.5"
  },
  "engines": {
    "node": ">=8.11.3 <9"
  },
  "scripts": {
    "start-server": "json-server --watch ./data/teams.json --read-only --no-cors --delay 1500 --port 3004",
    "start": "NODE_PATH=src react-scripts start & yarn start-server",
    "build": "NODE_PATH=src react-scripts build",
    "test": "NODE_PATH=src react-scripts test --env=jsdom",
    "eject": "NODE_PATH=src react-scripts eject",
    "precommit": "pretty-quick --staged"
  },
  "devDependencies": {
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0"
  }
}
